<dx:ThemedWindow x:Class="ToolMaster.MainWindow" mc:Ignorable="d" Title="Tool-Master" Height="450" Width="800" WindowState="Maximized" WindowStartupLocation="CenterScreen" x:Name="mainWindow"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:ToolMaster"
    Loaded="mainWindow_Loaded_1"
    xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui">
    <DockPanel>
        <dxwui:HamburgerMenu PaneBackground="Black" ViewState="Overlay">
            <dxwui:HamburgerMenu.Content>
                <dxwui:NavigationFrame />
            </dxwui:HamburgerMenu.Content>
            <!--<dxwui:HamburgerMenuNavigationButton x:Name="btnMain" Content="메인화면" GlyphHeight="16" GlyphWidth="16" IsSelected="True" Glyph="{dx:DXImage SvgImages/Icon Builder/Actions_Home.svg}" Click="btnMain_Click" FontSize="15" Visibility="Visible" />-->
            <!--<dxwui:HamburgerMenuItemSeparator />-->
            <!--<dxwui:HamburgerMenuNavigationButton x:Name="btnManageEsn" IsEnabled="False" Content="ESN 관리" GlyphHeight="16" GlyphWidth="16" Glyph="{dx:DXImage SvgImages/Reports/PrintAllPages.svg}" />-->
            <dxwui:HamburgerMenuNavigationButton x:Name="btnLog" IsEnabled="True" Content="EMS Log 관리" GlyphHeight="16" GlyphWidth="16" Glyph="{dx:DXImage SvgImages/PDF Viewer/Copy.svg}" Click="btnLog_Click"  FontSize="15"/>
            <dxwui:HamburgerMenuNavigationButton x:Name="btnEms" IsEnabled="True" Content="EMS 관리" GlyphHeight="16" GlyphWidth="16" Glyph="{dx:DXImage SvgImages/Outlook Inspired/PanelRight.svg}" Click="btnEms_Click" FontSize="15" />
            <dxwui:HamburgerMenuNavigationButton x:Name="btnManageEms" IsEnabled="True" Content="EMS DB 관리" GlyphHeight="16" GlyphWidth="16" Glyph="{dx:DXImage SvgImages/Business Objects/BO_Document.svg}" Click="btnManageEms_Click" FontSize="15" Background="{x:Null}" />
            <dxwui:HamburgerMenuNavigationButton x:Name="btnManageGateway" IsEnabled="True" Content="Gateway 관리" GlyphHeight="16" GlyphWidth="16" Glyph="{dx:DXImage SvgImages/Icon Builder/Electronics_Router.svg}" Click="btnManageGateway_Click" FontSize="15" />
            <dxwui:HamburgerMenuNavigationButton x:Name="btnExportFile" IsEnabled="True" Content="파일로 저장" GlyphHeight="16" GlyphWidth="16" Glyph="{dx:DXImage SvgImages/Export/ExportFile.svg}" Click="btnExportFile_Click"  FontSize="15" />
            <dxwui:HamburgerMenuNavigationButton x:Name="btnImportFile" IsEnabled="True" Content="파일 불러오기" GlyphHeight="16" GlyphWidth="16" Glyph="{dx:DXImage SvgImages/Outlook Inspired/AddFile.svg}" Click="btnImportFile_Click"  FontSize="15" />
            <dxwui:HamburgerMenuNavigationButton x:Name="btnRequestTest" IsEnabled="True" Content="RequestTest" GlyphHeight="16" GlyphWidth="16" Glyph="{dx:DXImage SvgImages/Outlook Inspired/AddFile.svg}" Click="btnRequestTest_Click"  FontSize="15" />
            <!--<dxwui:HamburgerMenuNavigationButton x:Name="btnMatching" IsEnabled="False" Content="1:1 매칭" GlyphHeight="16" GlyphWidth="16" Glyph="{dx:DXImage SvgImages/DiagramIcons/RightAngleConnector.svg}" FontSize="15" />-->
            <dxwui:HamburgerMenuItemSeparator />
            <dxwui:HamburgerMenuNavigationButton x:Name="btnSetting" IsEnabled="True" Content="설정" GlyphHeight="16" GlyphWidth="16" Glyph="{dx:DXImage SvgImages/Icon Builder/Actions_Settings.svg}" Click="btnSetting_Click" FontSize="15"/>
            <!--<dxwui:HamburgerMenuHyperlinkButton Content="Support" NavigateUri="https://www.devexpress.com/Support/"/>-->
        </dxwui:HamburgerMenu>
        <!--<Frame x:Name="frameMainWindow" HorizontalAlignment="Stretch" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch" Height="865" NavigationUIVisibility="Hidden" />-->
        <dxwui:NavigationFrame x:Name="frameMainWindow" HorizontalAlignment="Stretch" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch" Height="865"/>
    </DockPanel>

